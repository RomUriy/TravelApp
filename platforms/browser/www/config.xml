<?xml version='1.0' encoding='utf-8'?>
<widget id="TravelApp" version="1.2.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <splash src="img/logo.png" />
    <name>TravelApp</name>
    <description>TravelApp</description>
    <author email="romuriy@gmail.com" href="http://cemoe.com">
		Roman Yurkevych, Ihor Lobanov</author>
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
